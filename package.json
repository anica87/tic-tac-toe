{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "A TicTacToe game",
  "author": "Joblocal GmbH",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --inline --progress",
    "prebuild": "rm -rf dist/*",
    "build": "webpack -p --progress",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint . --ignore-path .gitignore"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-svg-loader": "^2.1.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.4"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "7.0.0-beta.54",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "7.0.0-beta.54",
    "@babel/runtime": "7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "17",
    "eslint-import-resolver-webpack": "0.10",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3",
    "jest": "^24.1.0",
    "jest-svg-transformer": "^1.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^@views(.*)$": "<rootDir>/src/views/$1",
      "^@components(.*)$": "<rootDir>/src/components/common/$1",
      "^@styles(.*)$": "<rootDir>/src/assets/styles/$1",
      "^@images(.*)$": "<rootDir>/src/assets/images/$1"
    }
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  }
}
